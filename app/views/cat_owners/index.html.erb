<p id="notice"><%= notice %></p>

<h1>Pet Owners</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Pets</th>
      <th>Actually a catgirl</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @pet_owners.each do |pet_owner| %>
      <tr>
        <td><%= pet_owner.name %></td>
        <td><%= pet_owner.pets %></td>
        <td><%= pet_owner.actually_a_catgirl %></td>
        <td><%= link_to 'Show', pet_owner %></td>
        <td><%= link_to 'Edit', edit_pet_owner_path(pet_owner) %></td>
        <td><%= link_to 'Destroy', pet_owner, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Pet Owner', new_pet_owner_path %>
